
class Solution:
    
    #Function to find two repeated elements.
    def twoRepeated(self, arr , N):
        #Your code here
        result=[]
        s=set()
        
        for i in range(len(arr)):
            if arr[i] in s:
                result.append(arr[i])
            else:
                s.add(arr[i])
        return result
