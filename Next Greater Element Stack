class Solution:
    def nextLargerElement(self,arr,n):
        #code here
        nge=[]
        stack=[]
        for i in range(len(arr)-1,-1,-1):
            currentval=arr[i]
            while(len(stack)!=0 and stack[-1]<=currentval):
                stack.pop()
            if(len(stack)==0):
                nge.append(-1)
            else:
                nge.append(stack[-1])
            stack.append(currentval)
        return nge[::-1]
