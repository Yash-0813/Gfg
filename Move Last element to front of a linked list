class Solution {
    public static Node moveToFront(Node head) {
        // code here
        Node temp=head;
        if(temp.next==null){
            return head;
        }
        while(temp.next.next!=null){
            temp=temp.next;
        }
        temp.next.next=head;
        head=temp.next;
        temp.next=null;
        return head;
    }
}
      
