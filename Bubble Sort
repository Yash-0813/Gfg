class Solution:
    #Function to sort the array using bubble sort algorithm.
    def bubbleSort(self,arr, n):
        # code here
        for j in range(0,n-1):
           for i in range(0,n-1):
               if arr[i]>arr[i+1]:
                   arr[i],arr[i+1]=arr[i+1],arr[i]
        return arr
